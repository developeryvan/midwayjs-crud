{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "author": "Yvan <yvan_chen@qq.com>",
  "scripts": {
    "start": "NODE_ENV=production node ./bootstrap.js",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "prisma generate && midway-bin build -c",
    "prisma:generate": "prisma generate",
    "prisma:init": "prisma db push && prisma db seed",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@casbin/redis-watcher": "^1.0.1",
    "@midwayjs/bootstrap": "^3.3.0",
    "@midwayjs/core": "^3.3.0",
    "@midwayjs/cross-domain": "^3.3.0",
    "@midwayjs/decorator": "^3.1.6",
    "@midwayjs/koa": "^3.3.0",
    "@midwayjs/redis": "^3.3.0",
    "@midwayjs/static-file": "^3.3.0",
    "@midwayjs/swagger": "^3.3.0",
    "@midwayjs/task": "^3.3.0",
    "@midwayjs/upload": "^3.3.0",
    "@midwayjs/validate": "^3.3.0",
    "@midwayjs/view-nunjucks": "^3.3.0",
    "@prisma/client": "^3.12.0",
    "@wecom/crypto": "^1.0.1",
    "axios": "^0.26.1",
    "axios-retry": "^3.2.4",
    "casbin": "^5.14.0",
    "casbin-prisma-adapter": "^1.1.3",
    "dayjs": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "nacos": "^2.2.1",
    "path-to-regexp": "^6.2.0",
    "sqlstring": "^2.3.3",
    "tunnel": "0.0.6",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.3.1",
    "@midwayjs/mock": "^3.3.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/sqlstring": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-decorator-position": "^4.0.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^27.5.1",
    "mwts": "^1.3.0",
    "prisma": "^3.12.0",
    "swagger-ui-dist": "^4.10.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts",
    "schema": "prisma/schema.prisma"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ]
}
